{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/send-location.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/send-location",
      "dest": "/api/send-location.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}